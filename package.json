{
  "name": "kirby-bucket",
  "version": "2.4.0",
  "description": "Kirby Bucket is a simple developer theme and boilerplate for the Kirby CMS.",
  "author": "Manuel Deutsch",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manueldeutsch/kirby-bucket.git"
  },
  "scripts": {
    "dev": "parcel watch ./assets/css/main.scss ./assets/js/main.js --dist-dir ./assets/dist --public-url .",
    "build": "parcel build --no-content-hash --no-source-maps ./assets/css/main.scss ./assets/js/main.js --dist-dir ./assets/dist --public-url ."
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@parcel/transformer-image": "^2.13.3",
    "@parcel/transformer-sass": "^2.13.3",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "parcel": "^2.8.0",
    "postcss": "^8.4.49",
    "sass": "^1.83.1"
  },
  "dependencies": {
    "modern-normalize": "^3.0.1"
  },
  "prettier": {
    "printWidth": 120
  }
}
